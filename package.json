{
  "name": "directus-extension-directus-sync",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-endpoint"
  ],
  "type": "module",
  "directus:extension": {
    "type": "endpoint",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^10.1.13"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,gql,mjs,cjs}\"",
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "test": "jest"
  },
  "devDependencies": {
    "@directus/errors": "^0.2.0",
    "@directus/extensions-sdk": "10.1.13",
    "@directus/random": "^0.2.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "sqlite3": "^5.1.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  }
}
